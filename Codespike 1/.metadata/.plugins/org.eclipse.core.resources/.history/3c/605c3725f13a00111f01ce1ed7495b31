package Liza;

import java.io.IOException;
import java.net.Socket;
import java.net.UnknownHostException;

public class LizaPlayer {
	
	Socket s;
	
	public LizaPlayer() {
		
	}
	
	public void connect() {
		try {
			s = new Socket("localhost", 25565);
		} catch (UnknownHostException e) {
			
		} catch (IOException e) {
			
		}
	}
	
	public void disconnect() {
		try {
			s.close();
		} catch (IOException e) {
			
		}
	}
}
